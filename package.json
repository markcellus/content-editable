{
    "name": "content-editable",
    "version": "1.2.0",
    "description": "An HTML element that makes its content editable",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/mkay581/content-editable/issues"
    },
    "homepage": "https://github.com/mkay581/content-editable#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mkay581/content-editable.git"
    },
    "keywords": [
        "content editable component",
        "content editable",
        "content editable element",
        "editable input element",
        "editable element",
        "editable webcomponent",
        "editable textfield component",
        "inline edit component",
        "inline edit",
        "inline",
        "editable",
        "edit text field"
    ],
    "author": "Mark Kennedy <mark@heyimmark.com> (heyimmark.com)",
    "engines": {
        "node": ">=10"
    },
    "main": "dist/content-editable.js",
    "types": "dist/content-editable.d.ts",
    "scripts": {
        "test": "karma start karma.conf.js && npm run lint",
        "test-server": "karma start karma.conf.js --single-run=false",
        "preversion": "npm test",
        "banner": "banner-cli dist/*.js",
        "build": "rm -rf dist/* && rollup -c",
        "version": "npm run build && npm run banner && git add -A dist",
        "postversion": "git push && git push --tags",
        "build:dev": "rollup -c --watch --dev",
        "start": "run-p build:dev serve",
        "serve": "servor examples --reload --browse",
        "lint": "npm run lint:scripts && npm run lint:format",
        "lint:scripts": "tslint src/**/*",
        "lint:format": "prettier --check '**/*'",
        "coveralls": "cat ./coverage/lcov.info | coveralls"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/mocha": "^8.0.3",
        "@types/sinon": "^9.0.0",
        "banner-cli": "0.14.3",
        "chai": "^4.2.0",
        "husky": "^5.1.1",
        "karma": "^6.1.1",
        "karma-chai": "^0.1.0",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "^2.0.2",
        "karma-mocha": "^2.0.0",
        "karma-typescript": "^5.0.1",
        "mocha": "^8.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.2",
        "pretty-quick": "^3.0.0",
        "rollup": "^2.1.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-terser": "^7.0.0",
        "rollup-plugin-typescript2": "^0.30.0",
        "servor": "^4.0.2",
        "sinon": "^9.0.1",
        "tslint": "^6.1.0",
        "tslint-config-prettier": "^1.15.0",
        "typescript": "^4.2.2"
    },
    "dependencies": {
        "anchorme": "^1.1.2"
    }
}
